graph g {
	NAME [shape=box]
	BINDING [shape=box]
	"TRANSCRIPTION FACTOR" [shape=box]
	GENE [shape=box]
	"IS CALLED" [shape=diamond]
	"IS BOUND" [shape=diamond]
	TRANSCRIBES [shape=diamond]

	transcription_name [label=<<u>name</u>>]
	"TRANSCRIPTION FACTOR" -- transcription_name
	GENE -- "IS BOUND" [label="1"]
	"IS BOUND" -- BINDING [label="1..n"]
	BINDING -- TRANSCRIBES [label="1"]
	TRANSCRIBES -- "TRANSCRIPTION FACTOR" [label="1..n"]

	gene_sequence [label=<<u>sequence</u>>]
	gene_sequence -- GENE

	binding_sequence [label=<<u>binding sequence</u>>]
	BINDING -- binding_sequence

	gene_name[label=<<u>gene name</u>>]
	gene_name -- NAME
	GENE -- "IS CALLED" [label="1"]
	"IS CALLED" -- NAME [label="1..n"]
}
